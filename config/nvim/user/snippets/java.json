  "ArrayInput": {
    "prefix": "ainp",
    "body": [
      "Scanner sc = new Scanner(System.in);",
      "System.out.print(\"Enter Array Length: \");",
      "int n = sc.nextInt();",
      "System.out.print(\"Enter Array Elements: \");",
      "int[] a = new int[n];",
      "for(int i = 0;i < n;i++) {",
      "  a[i] = sc.nextInt();",
      "}"
    ],
    "description": "Array Input Method"
  },
  "ArrayPrint": {
    "prefix": "apri",
    "body": [
      "for(int i = 0;i < a.length;i++) {",
      "  System.out.print(a[i] + \" \");",
      "}"
    ]
  }
